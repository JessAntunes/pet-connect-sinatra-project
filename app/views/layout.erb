  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1" />

    <title>PetConnect</title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link rel="stylesheet" href="/stylesheets/main.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
  </head>
  <body>
   
    <div class="wrapper">
    <nav>
    <div class="light-blue lighten-2 nav-wrapper">
      <img src="images/Small-logo-cat.png">
      <a href="/" class="brand-logo">  PetConnect</a>
      <ul id="nav-mobile" class="right light-blue lighten-2 hide-on-med-and-down">
        <%if logged_in?%>
        <li><a href="/logout">Logout</a></li>
        <%else%>
        <li><a href="/login">Login</a></li>
        <%end%>
      </ul>
    </div>
    </nav>

    <% flash.keys.each do | x | %>
      <h6><%=flash[x]%></h6>
    <% end %>
      
      
        <%= yield %>

      <footer class="branding">
        <small>&copy; 2021 <strong>PetConnect</strong></small>
      </footer>
    </div>

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
    <!--[if lt IE 7]>
      <script src="//ajax.googleapis.com/ajax/libs/chrome-frame/1.0.2/CFInstall.min.js"></script>
      <script>window.attachEvent("onload",function(){CFInstall.check({mode:"overlay"})})</script>
    <![endif]-->
  </body>
</html>
